{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sap.ui.study.git",
		"type": "application",
		"i18n": "models/i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"employeeRemote": {
				"uri": "/here/goes/your/serviceUrl/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sap.ui.study.git.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.30",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sap.ui.study.git.models.i18n.i18n"
				}
			},
			"": {
				"dataSource": "employeeRemote"
			},
			"sampleMenu": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "models/sampleMenu.json",
				"settings": {},
				"preload": false
			},
			"studyMenu": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "models/studyMenu.json",
				"preload": false
			},
			"codeModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "models/codeModel.json",
				"preload": true
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "sap.ui.study.git.view",
				"controlId": "container",
				"controlAggregation": "detailPages",
				"transition": "flip",
				"bypassed": {
					"target": ["notFound"]
				},
				"async": true,
				"viewLevel": 1
			},
			"routes": [{
				"name": "homePage",
				"pattern": "",
				"titleTarget": "",
				"greedy": false,
				"target": ["homePage"]
			}, {
				"name": "samplePage",
				"pattern": "sample",
				"titleTarget": "",
				"greedy": false,
				"target": ["sampleMenu", "samplePage"]
			}, {
				"name": "listExample",
				"pattern": "sample/listbase/list",
				"titleTarget": "",
				"greedy": false,
				"target": ["listExample", "sampleMenu"]
			}, {
				"name": "tableExample",
				"pattern": "sample/listbase/table",
				"titleTarget": "",
				"greedy": false,
				"target": ["sampleMenu", "tableExample"]
			}, {
				"name": "blockLayoutExample",
				"pattern": "sample/layout/block_layout",
				"titleTarget": "",
				"greedy": false,
				"target": ["blockLayoutExample", "sampleMenu"]
			}, {
				"name": "formExample",
				"pattern": "sample/layout/form",
				"titleTarget": "",
				"greedy": false,
				"target": ["formExample", "sampleMenu"]
			}, {
				"name": "gridExample",
				"pattern": "sample/layout/grid",
				"titleTarget": "",
				"greedy": false,
				"target": ["gridExample", "sampleMenu"]
			}, {
				"name": "responsiveSplitterExample",
				"pattern": "sample/layout/responsive_splitter",
				"titleTarget": "",
				"greedy": false,
				"target": ["sampleMenu", "panelOne", "panelTwo", "panelThree"]
			}, {
				"name": "studyPage",
				"pattern": "study",
				"titleTarget": "",
				"greedy": false,
				"target": ["studyMenu", "studyPage"]
			}],
			"targets": {
				"notFound": {
					"viewId": "notFound",
					"viewName": "NotFound",
					"transition": "show",
					"controlAggregation": "detailPages",
					"viewLevel": 1
				},
				"sampleMenu": {
					"viewType": "XML",
					"transition": "show",
					"clearControlAggregation": false,
					"viewName": "SampleMenu",
					"viewId": "sampleMenu",
					"viewLevel": 1,
					"controlAggregation": "masterPages"
				},
				"samplePage": {
					"viewType": "XML",
					"transition": "show",
					"clearControlAggregation": false,
					"viewName": "SamplePage",
					"viewId": "samplePage",
					"viewLevel": 1,
					"controlAggregation": "detailPages"
				},
				"homePage": {
					"viewType": "XML",
					"transition": "show",
					"clearControlAggregation": false,
					"viewName": "HomePage",
					"viewId": "homePage",
					"viewLevel": 1,
					"controlAggregation": "detailPages"
				},
				"studyPage": {
					"viewType": "XML",
					"transition": "show",
					"clearControlAggregation": false,
					"viewName": "StudyPage",
					"viewId": "studyPage",
					"viewLevel": 1,
					"controlAggregation": "detailPages"
				},
				"studyMenu": {
					"viewType": "XML",
					"transition": "show",
					"clearControlAggregation": false,
					"viewName": "StudyMenu",
					"viewId": "studyMenu",
					"viewLevel": 1,
					"controlAggregation": "masterPages"
				},
				"listExample": {
					"viewType": "XML",
					"transition": "show",
					"clearControlAggregation": true,
					"viewName": "ListExample",
					"viewId": "listExample",
					"viewLevel": 3,
					"controlAggregation": "pages",
					"viewPath": "sap.ui.study.git.view.list",
					"controlId": "navContainer",
					"parent": "detailHeader"
				},
				"tableExample": {
					"viewType": "XML",
					"transition": "show",
					"clearControlAggregation": true,
					"viewName": "TableExample",
					"viewId": "tableExample",
					"viewLevel": 3,
					"controlId": "navContainer",
					"controlAggregation": "pages",
					"viewPath": "sap.ui.study.git.view.list",
					"parent": "detailHeader"
				},
				"blockLayoutExample": {
					"viewType": "XML",
					"transition": "show",
					"clearControlAggregation": true,
					"viewName": "BlockLayoutExample",
					"viewId": "blockLayoutExample",
					"viewLevel": 3,
					"controlAggregation": "pages",
					"viewPath": "sap.ui.study.git.view.layout",
					"controlId": "navContainer",
					"parent": "detailHeader"
				},
				"formExample": {
					"viewType": "XML",
					"transition": "show",
					"clearControlAggregation": true,
					"viewName": "FormExample",
					"viewId": "formExample",
					"viewLevel": 3,
					"controlAggregation": "pages",
					"viewPath": "sap.ui.study.git.view.layout",
					"controlId": "navContainer",
					"parent": "detailHeader"
				},
				"gridExample": {
					"viewType": "XML",
					"transition": "show",
					"clearControlAggregation": true,
					"viewName": "GridExample",
					"viewId": "gridExample",
					"viewLevel": 3,
					"controlAggregation": "pages",
					"viewPath": "sap.ui.study.git.view.layout",
					"parent": "detailHeader",
					"controlId": "navContainer"
				},
				"responsiveSplitterExample": {
					"viewType": "XML",
					"transition": "show",
					"clearControlAggregation": "",
					"viewName": "ResponsiveSplitterExample",
					"viewId": "responsiveSplitterExample",
					"viewLevel": 2,
					"controlAggregation": "detailPages",
					"viewPath": "sap.ui.study.git.view.layout"
				},
				"detailHeader": {
					"viewType": "XML",
					"transition": "show",
					"clearControlAggregation": false,
					"viewName": "DetailHeader",
					"viewId": "detailHeader",
					"viewLevel": 2,
					"controlAggregation": "detailPages"
				},
				"panelOne": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": true,
					"viewName": "FlexBox",
					"viewId": "flexBox",
					"viewLevel": 3,
					"controlAggregation": "content",
					"controlId": "panelOne",
					"viewPath": "sap.ui.study.git.view.layout",
					"parent": "responsiveSplitterExample"
				},
				"panelTwo": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": true,
					"viewName": "FormForSplitterExample",
					"viewId": "formforSplitterExample",
					"viewLevel": 3,
					"controlAggregation": "content",
					"controlId": "panelTwo",
					"viewPath": "sap.ui.study.git.view.layout",
					"parent": "responsiveSplitterExample"
				},
				"panelThree": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": true,
					"viewName": "ListExample",
					"viewPath": "sap.ui.study.git.view.list",
					"controlId": "panelThree",
					"controlAggregation": "content",
					"viewLevel": 3,
					"viewId": "listExample",
					"parent": "responsiveSplitterExample"
				},
				"codeEditor": {
					"viewType": "XML",
					"transition": "flip",
					"clearControlAggregation": false,
					"viewName": "CodeEditor",
					"viewId": "codeEditor",
					"viewLevel": 5,
					"controlAggregation": "pages",
					"controlId": "navContainer",
					"parent": "detailHeader"
				}
			}
		},
		"contentDensities": {
			"compact": false,
			"cozy": false
		},
		"flexEnabled": true
	}
}