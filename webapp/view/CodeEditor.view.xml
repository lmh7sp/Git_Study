<mvc:View
	controllerName="sap.ui.study.git.controller.CodeEditor"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:ce="sap.ui.codeeditor">
	<IconTabHeader id="iconTabHeader"
		selectedKey="invalidKey"
		select="onSelectCode">
		<items>
			<IconTabFilter text="View" key="A"></IconTabFilter>
			<IconTabFilter text="Controller" key="B"></IconTabFilter>
		</items>
	</IconTabHeader>
	<ce:CodeEditor id="codeEditor" height="100%" syntaxHints="false" type="javascript" />
</mvc:View>
