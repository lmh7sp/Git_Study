<mvc:View
	controllerName="sap.ui.study.git.controller.Home"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page title="{i18n>homePageTitle}" class="sapUiResponsiveContentPadding">
		<content>
			<Panel headerText="항목별 예제">
				<content>
					<Button id="tableExampleBtn" text="{i18n>ShowTable}" press=".onNavToTable" class="sapUiTinyMarginEnd"/>
				</content>
			</Panel>
			<Panel headerText="업무별 예제">
				<content>
					<Button text="{i18n>ShowTable}" press=".onNavToTable" class="sapUiTinyMarginEnd"/>
				</content>
			</Panel>
		</content>
	</Page>
</mvc:View>
